<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Theme.ProxyLauncher" parent="@android:style/Theme.Translucent.NoTitleBar" />
    <style name="Theme.Camera" parent="@android:style/Theme.Holo.Light.DarkActionBar">
        <item name="android:colorBackground">@color/camera_gray_background</item>
        <item name="android:windowBackground">@color/camera_gray_background</item>
        <item name="android:windowTitleSize">0.0dip</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:colorBackgroundCacheHint">@color/camera_gray_background</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:actionBarStyle">@style/Camera.ActionBar</item>
        <item name="android:windowActionBarOverlay">true</item>
    </style>
    <style name="Theme.SecureCamera" parent="@style/Theme.Camera">
        <item name="android:windowBackground">@drawable/cover</item>
    </style>
    <style name="Camera.ActionBar" parent="@android:style/Widget.Holo.Light.ActionBar">
        <item name="android:background">@drawable/actionbar_translucent</item>
        <item name="android:displayOptions">homeAsUp|showHome|useLogo</item>
        <item name="android:backgroundStacked">@null</item>
    </style>
    <style name="OnScreenHintTextAppearance">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@android:color/primary_text_dark</item>
        <item name="android:textColorHighlight">#ffff9200</item>
        <item name="android:textColorHint">#ff808080</item>
        <item name="android:textColorLink">#ff5c5cff</item>
    </style>
    <style name="OnScreenHintTextAppearance.Small" parent="@style/OnScreenHintTextAppearance">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@android:color/secondary_text_dark</item>
    </style>
    <style name="Animation_OnScreenHint">
        <item name="android:windowEnterAnimation">@anim/on_screen_hint_enter</item>
        <item name="android:windowExitAnimation">@anim/on_screen_hint_exit</item>
    </style>
    <style name="ReviewPlayIcon">
        <item name="android:visibility">gone</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:src">@drawable/ic_gallery_play_big</item>
        <item name="android:layout_centerInParent">true</item>
    </style>
    <style name="PopupTitleSeparator">
        <item name="android:background">@color/popup_title_color</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">2.0dip</item>
    </style>
    <style name="OnViewfinderLabel">
        <item name="android:textSize">16.0dip</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/bg_text_on_preview</item>
        <item name="android:paddingLeft">15.0dip</item>
        <item name="android:paddingTop">3.0dip</item>
        <item name="android:paddingRight">15.0dip</item>
        <item name="android:paddingBottom">3.0dip</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">10.0dip</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="PanoCustomDialogText">
        <item name="android:textAppearance">@android:style/TextAppearance.Medium</item>
    </style>
    <style name="TextAppearance.DialogWindowTitle" parent="">
        <item name="android:textSize">22.0sp</item>
        <item name="android:textColor">@color/holo_blue_light</item>
    </style>
    <style name="TextAppearance.Medium" parent="@android:style/TextAppearance.Medium" />
    <style name="Widget.Button.Borderless" parent="@android:style/Widget.Button">
        <item name="android:textAppearance">@style/TextAppearance.Medium</item>
        <item name="android:textColor">@color/primary_text</item>
        <item name="android:background">@drawable/bg_pressed</item>
        <item name="android:paddingLeft">4.0dip</item>
        <item name="android:paddingRight">4.0dip</item>
        <item name="android:minWidth">64.0dip</item>
        <item name="android:minHeight">48.0dip</item>
    </style>
    <style name="ReviewControlText_xlarge">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/bg_pressed_exit_fading</item>
        <item name="android:paddingLeft">2.0dip</item>
        <item name="android:paddingTop">10.0dip</item>
        <item name="android:paddingRight">10.0dip</item>
        <item name="android:paddingBottom">10.0dip</item>
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="PopupTitleText_xlarge">
        <item name="android:textSize">@dimen/popup_title_text_size</item>
        <item name="android:textColor">@color/popup_title_color</item>
        <item name="android:layout_gravity">center|left</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">10.0dip</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="PanoCustomDialogText_xlarge">
        <item name="android:textAppearance">@android:style/TextAppearance.Large</item>
    </style>
    <style name="ViewfinderLabelLayout_xlarge">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_margin">13.0dip</item>
    </style>
    <style name="SwitcherButton">
        <item name="android:background">@drawable/bg_pressed_exit_fading</item>
        <item name="android:layout_width">@dimen/switcher_size</item>
        <item name="android:layout_height">@dimen/switcher_size</item>
    </style>
    <style name="MenuIndicator">
        <item name="android:enabled">false</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:scaleType">center</item>
    </style>
    <style name="CameraControls">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
    </style>
    <style name="UndoBar">
        <item name="android:layout_gravity">bottom|center</item>
        <item name="android:background">@drawable/panel_undo_holo</item>
        <item name="android:paddingLeft">16.0dip</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">48.0dip</item>
        <item name="android:layout_marginBottom">64.0dip</item>
    </style>
    <style name="UndoBarTextAppearance">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">@android:color/white</item>
    </style>
    <style name="UndoBarSeparator">
        <item name="android:background">@color/gray</item>
        <item name="android:paddingRight">12.0dip</item>
        <item name="android:layout_width">1.0dip</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_marginTop">10.0dip</item>
        <item name="android:layout_marginBottom">10.0dip</item>
    </style>
    <style name="UndoButton">
        <item name="android:textSize">12.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/gray</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:background">@drawable/bg_pressed</item>
        <item name="android:paddingRight">16.0dip</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:drawablePadding">8.0dip</item>
    </style>
    <style name="PanoViewHorizontalBar">
        <item name="android:background">#ff000000</item>
        <item name="android:alpha">1.0</item>
    </style>
    <style name="FilmstripBottomControlButton">
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:background">@drawable/transparent_button_background</item>
        <item name="android:layout_width">@dimen/filmstrip_bottom_control_button_size</item>
        <item name="android:layout_height">@dimen/filmstrip_bottom_control_button_size</item>
        <item name="android:scaleType">centerInside</item>
    </style>
    <style name="FilmstripBottomProgressText">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:shadowColor">@android:color/black</item>
        <item name="android:shadowDx">0.0</item>
        <item name="android:shadowDy">0.0</item>
        <item name="android:shadowRadius">2.0</item>
    </style>
    <style name="IndicatorIcon">
        <item name="android:background">@null</item>
        <item name="android:visibility">gone</item>
        <item name="android:layout_width">@dimen/indicator_icon_width</item>
        <item name="android:layout_height">@dimen/indicator_icon_height</item>
    </style>
    <style name="ModeOption">
        <item name="android:background">@null</item>
        <item name="android:layout_width">@dimen/mode_option_width</item>
        <item name="android:layout_height">@dimen/mode_option_height</item>
        <item name="android:layout_margin">6.0dip</item>
    </style>
    <style name="Theme.CameraSettings" parent="@android:style/Theme.Holo.Light.DarkActionBar">
        <item name="android:windowFullscreen">true</item>
        <item name="android:actionBarStyle">@style/Settings.ActionBar</item>
    </style>
    <style name="Settings.ActionBar" parent="@android:style/Widget.Holo.Light.ActionBar.Solid.Inverse">
        <item name="android:background">@color/main_color_global</item>
    </style>
    <style name="start_up_dialog_content">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
    </style>
    <style name="fullscreen_dialog_title">
        <item name="android:textSize">24.0sp</item>
        <item name="android:textColor">@color/dialog_text_color</item>
        <item name="android:fontFamily">sans-serif-regular</item>
    </style>
    <style name="BlueButton">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">@color/blue_button_text_color</item>
        <item name="android:background">@drawable/button_cling</item>
        <item name="android:padding">12.0dip</item>
        <item name="android:layout_width">144.0dip</item>
        <item name="android:layout_height">48.0dip</item>
        <item name="android:fontFamily">sans-serif-medium</item>
    </style>
    <style name="DialogCheckBox">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">@color/dialog_text_color</item>
        <item name="android:button">@drawable/checkbox_material_black</item>
        <item name="android:lineSpacingExtra">1.8999939sp</item>
    </style>
</resources>
